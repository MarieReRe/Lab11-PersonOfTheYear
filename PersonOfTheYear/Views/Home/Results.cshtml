
@model List<PersonOfTheYear.Models.TimePerson>


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>The Person of The Year</title>
    <link rel="stylesheet" href="~/css/StyleSheet.css" />
</head>
<body>
    <header>
        <nav class="headerNav">
            <ul>
                <li><a href="#">Home</a></li>
                <li><a href="#">About This App</a></li>
                <li><a href="#">About The Developer</a></li>
                <li><a href="..">Search Again</a></li>
            </ul>
        </nav>
    </header>

<main>
    <h1>Your Search Results</h1>
    
    <div>
        <table>
            <tr>
                <th>Year</th>
                <th>Honor</th>
                <th>Name</th>
                <th>Country</th>
                <th>BirthYear</th>
                <th>DeathYear</th>
                <th>Title</th>
                <th>Category</th>
                <th>Context</th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Year</td>
                    <td>@item.Honor</td>
                    <td>@item.Name</td>
                    <td>@item.Country</td>
                    <td>@item.BirthYear</td>
                    <td>@item.DeathYear</td>
                    <td>@item.Title</td>
                    <td>@item.Category</td>
                    <td>@item.Context</td>
                </tr>
            }

        </table>
    </div>
    
    
    
    

</main>





</body>
</html>